{
  "name": "tic-tac-toe-client",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://mattcroberts.github.io/tic-tac-toe",
  "scripts": {
    "generate-types": "gql-gen --schema http://localhost:3000/graphql --template graphql-codegen-typescript-template --out ./typings/ \"./src/**/*.ts\"",
    "start": "parcel -p 3001 public/index.html",
    "prebuild": "rm -rf dist",
    "build": "parcel build --public-url https://mattcroberts.github.io/tic-tac-toe/ public/index.html",
    "test": "jest",
    "lint": "tslint -t codeFrame 'src/**/*.ts'",
    "deploy": "gh-pages -d dist"
  },
  "dependencies": {
    "@types/react-router-dom": "^4.3.0",
    "@types/recompose": "^0.26.1",
    "apollo-cache-inmemory": "^1.2.1",
    "apollo-client": "^2.3.1",
    "apollo-link-http": "^1.5.4",
    "gh-pages": "^1.2.0",
    "graphql": "^0.13.2",
    "graphql-tag": "^2.9.2",
    "history": "^4.7.2",
    "lodash": "^4.17.10",
    "react": "^16.3.2",
    "react-apollo": "^2.1.4",
    "react-dom": "^16.3.2",
    "react-router-dom": "^4.3.1",
    "recompose": "^0.27.1",
    "whatwg-fetch": "2.0.3"
  },
  "devDependencies": {
    "@types/graphql": "^0.13.1",
    "@types/jest": "^23.3.1",
    "@types/lodash": "^4.14.109",
    "@types/node": "^9.6.6",
    "@types/prop-types": "^15.5.3",
    "@types/react": "^16.4.11",
    "@types/react-dom": "^16.0.7",
    "@types/react-router": "^4.0.30",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "cssnano": "^4.1.0",
    "graphql-code-generator": "^0.9.1",
    "graphql-codegen-typescript-template": "^0.9.1",
    "jest": "^23.5.0",
    "jest-transform-stub": "^1.0.0",
    "parcel-bundler": "^1.9.7",
    "ts-jest": "^23.1.4",
    "tslint": "^5.7.0",
    "tslint-config-prettier": "^1.10.0",
    "tslint-react": "^3.2.0",
    "typescript": "^2.8.3"
  }
}
